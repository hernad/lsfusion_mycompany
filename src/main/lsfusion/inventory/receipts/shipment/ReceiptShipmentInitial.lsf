MODULE ReceiptShipmentInitial;

REQUIRE ReceiptShipment, ShipmentPeriod;

NAMESPACE Inventory;

loadDefaultData () + {
    NEW s = ShipmentType {
        name(s) <- 'Return to Vendor';
        id(s) <- 'return';
        isReturn(s) <- TRUE;
        nonperiodic(s) <- TRUE;
        
        shipmentType(ReceiptType t) <- s WHERE id(t) = 'receipt';
    }
}

