        NEW tabbedPane {
            type = TABBED;
            fill = 1;
            NEW commons {
                caption = 'Basic';
            }
        }
    }
} 
 

NAVIGATOR {
    NEW FOLDER sales 'Sales' WINDOW toolbar IMAGE 'sales.png' {
        NEW FOLDER operations 'Actions' {  
        }
        NEW FOLDER dashboards 'Processes' {  
        }
        NEW FOLDER reporting 'Reports' {
        }
        NEW FOLDER settings 'Customization' {
            NEW options;        
        }
    }
}
